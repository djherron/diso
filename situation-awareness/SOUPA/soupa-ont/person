       </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#fax"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#homepage"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#email"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>    

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#mobile"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#im"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#myagent"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
