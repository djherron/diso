  <owl:Class rdf:ID="Homepage">
    <rdfs:label>Homepage</rdfs:label>
    <rdfs:subClassOf rdf:resource="#InternetResourceURL"/>
    <rdfs:subClassOf rdf:resource="#Document"/>
  </owl:Class>

  <owl:Class rdf:ID="Email">
    <rdfs:label>Email</rdfs:label>
    <rdfs:subClassOf rdf:resource="#InternetResourceURL"/>
