  <owl:Class rdf:ID="ContactProfile">
    <rdfs:label>ContactProfile</rdfs:label>

    <rdfs:subClassOf>
       <owl:Restriction>
          <owl:onProperty rdf:resource="#address"/>
          <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
       </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
