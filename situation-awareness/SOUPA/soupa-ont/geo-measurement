  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:ID="direction">
    <rdfs:label>direction</rdfs:label>
    <rdfs:domain rdf:resource="#DistanceValueDescription"/>
    <rdfs:range rdf:resource="#Direction"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="DistanceUnit">
