<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
   <!ENTITY cobra  "http://cobra.umbc.edu/ont/2004/05/">   
   <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#"> 
   <!ENTITY owl    "http://www.w3.org/2002/07/owl#"> 
   <!ENTITY per    "http://pervasive.semanticweb.org/ont/dev/person#">
   <!ENTITY agt    "http://pervasive.semanticweb.org/ont/dev/agent#">
   <!ENTITY spc    "http://pervasive.semanticweb.org/ont/dev/space#">
   <!ENTITY mtg    "http://pervasive.semanticweb.org/ont/dev/meeting#">
   <!ENTITY ddc    "http://pervasive.semanticweb.org/ont/dev/digital-doc#">
   <!ENTITY ebm    "http://cobra.umbc.edu/ont/2004/05/ebiquity-meetings#">
   <!ENTITY ebgeo  "http://cobra.umbc.edu/ont/2004/05/ebiquity-geo#">   
   <!ENTITY foaf   "http://xmlns.com/foaf/0.1#">
   ]>
