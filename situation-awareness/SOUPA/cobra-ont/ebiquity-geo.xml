<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
   <!ENTITY cobra  "http://cobra.umbc.edu/ont/2004/05/">   
   <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#"> 
   <!ENTITY owl    "http://www.w3.org/2002/07/owl#"> 
   <!ENTITY spc    "http://pervasive.semanticweb.org/ont/dev/space#">
   <!ENTITY rcc    "http://pervasive.semanticweb.org/ont/dev/rcc#">
   <!ENTITY geom   "http://pervasive.semanticweb.org/ont/dev/geo-measurement#">
   <!ENTITY ebgeo  "http://cobra.umbc.edu/ont/2004/05/ebiquity-geo#">
   ]>
