  </Literature>
  <Literature rdf:ID="DDW99">
    <hasQuotation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Herve Debar, Marc Dacier, Andreas Wespi. "Towards a taxonomy of intrusion-detection systems". Computer Networks. Vol. 31. Pages: 805-822. Elsevier Science B.V. 1999.</hasQuotation>
  </Literature>
  <Literature rdf:ID="ODotI">
    <hasQuotation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Oxford Dictionary of the Internet. Oxford University Press. http://www.oxfordreference.com</hasQuotation>
  </Literature>
  <Literature rdf:ID="Wikipedia">
    <hasQuotation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://en.wikipedia.org</hasQuotation>
  </Literature>
  <Literature rdf:ID="Gol99">
    <hasQuotation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Dieter Gollmann. Computer Security. Wiley &amp; Sons. 1999.</hasQuotation>
  </Literature>
</rdf:RDF>

