<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
  xml:base="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An ontology for security literature.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="Literature">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Dictionaries and other resources used as sources for the definitions of classes.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Definition">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasSource"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#Literature"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A class that contains definitions from literature.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasSource"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#hasSource">
    <rdfs:domain rdf:resource="#Definition"/>
    <rdfs:range rdf:resource="#Literature"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasQuotation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Literature"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasDefinition">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >contains the definition.</rdfs:comment>
    <rdfs:domain rdf:resource="#Definition"/>
  </owl:DatatypeProperty>
  <Literature rdf:ID="Szo05">
    <hasQuotation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Peter Szor. The Art of Computer Virus Research and Defense. Addison Wesley. 2005.</hasQuotation>
  </Literature>
  <Literature rdf:ID="Bis03">
    <hasQuotation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Matt Bishop. Computer Security---Art and Science. Addison Wesley. 2003.</hasQuotation>
  </Literature>
  <Literature rdf:ID="Pho02">
