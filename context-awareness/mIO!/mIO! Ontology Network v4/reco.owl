    <owl:Ontology rdf:about="">
        <dc:date>2008-11-13</dc:date>
        <dc:creator>Luis Polo</dc:creator>
        <dc:contributor
            >Sergio Fern&#225;ndez</dc:contributor>
        <dc:contributor>Diego Berrueta</dc:contributor>
        <dc:creator>Emilio Rubiera</dc:creator>
        <dc:rights
            >(c) TreeLogic, 2009</dc:rights>
        <dc:creator>Ivan M&#237;nguez</dc:creator>
        <dc:title xml:lang="en">RECO Ontology</dc:title>
        <dc:description xml:lang="en"
            >RECommendations Ontology: a vocabulary to formalize preferences in the Semantic Web</dc:description>
    </owl:Ontology>
    


