    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#andComposite -->

    <owl:ObjectProperty rdf:about="#andComposite"/>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#andComposition -->

    <owl:ObjectProperty rdf:about="#andComposition">
        <rdfs:label xml:lang="en">AND Composition</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A property which matches each composite-preference with the set of preferences composing it. Being an and-Composition, all the preferences have to be realised.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#CompositePreference"/>
                    <rdf:Description rdf:about="&mIO;CompositeDependency"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Role;Preference"/>
                    <rdf:Description rdf:about="&mIO;Dependency"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#asksFor -->

    <owl:ObjectProperty rdf:about="#asksFor">
        <rdfs:label xml:lang="en">Asks For</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The relationship between a user profile, agent or a group of agents and their demands, which can be mandatory, positive, negative or exclusive.</rdfs:comment>
        <rdfs:range rdf:resource="#Demand"/>
        <rdfs:subPropertyOf rdf:resource="#internallyRepresents"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Profile"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                    <rdf:Description rdf:about="&foaf;Group"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#desireTowards -->

    <owl:ObjectProperty rdf:about="#desireTowards">
        <rdfs:label xml:lang="en">Desire Towards</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A modal relation expressing that a user has a strong preference towards something.</rdfs:comment>
        <rdfs:range rdf:resource="#Preference"/>
        <rdfs:subPropertyOf rdf:resource="#internallyRepresents"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Profile"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                    <rdf:Description rdf:about="&foaf;Group"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#excludedPreference -->

    <owl:ObjectProperty rdf:about="#excludedPreference">
        <rdfs:label xml:lang="en"
            >Excluded Preference</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property relates a demand with a particular condition P. Individuals that fulfill this condition P are excluded from the set of valid candidates for this demand.</rdfs:comment>
        <rdfs:domain rdf:resource="#Demand"/>
        <rdfs:range rdf:resource="#Preference"/>
        <rdfs:subPropertyOf rdf:resource="#refinedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#filter -->

    <owl:ObjectProperty rdf:about="#filter">
        <rdfs:label xml:lang="en">Filter</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A property which develops the pattern restraining the value of a datatype property.</rdfs:comment>
        <rdfs:range rdf:resource="#Filter"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Pattern"/>
                    <rdf:Description rdf:about="&Role;Preference"/>
                    <rdf:Description rdf:about="&mIO;Dependency"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#internallyRepresents -->

    <owl:ObjectProperty rdf:about="#internallyRepresents">
        <rdfs:label xml:lang="en"
            >Internally Represents</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A user internally represents (in her mind) a modal description: a desire, a plan, a duty, etc.</rdfs:comment>
        <rdfs:range rdf:resource="#ModalDescription"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Profile"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                    <rdf:Description rdf:about="&foaf;Group"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#negativePreference -->

    <owl:ObjectProperty rdf:about="#negativePreference">
        <rdfs:label xml:lang="en"
            >Negative Optional Preference</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property relates a demand with a particular condition P. Individuals that fulfill this condition P decline their utility value.</rdfs:comment>
        <rdfs:domain rdf:resource="#Demand"/>
        <rdfs:range rdf:resource="#Preference"/>
        <rdfs:subPropertyOf rdf:resource="#refinedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#notDesireTowards -->

    <owl:ObjectProperty rdf:about="#notDesireTowards">
        <rdfs:label xml:lang="en"
            >Not Desire Towards</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A modal relation expressing the absence of a preference towards something.</rdfs:comment>
        <rdfs:range rdf:resource="#Preference"/>
        <rdfs:subPropertyOf rdf:resource="#internallyRepresents"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&foaf;Agent"/>
                    <rdf:Description rdf:about="&foaf;Group"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#operator -->

    <owl:ObjectProperty rdf:about="#operator">
        <rdfs:label xml:lang="en">Operator</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A property which matches a pattern to the operator restraining the value of a datatype property.</rdfs:comment>
        <rdfs:domain rdf:resource="#Filter"/>
        <rdfs:range rdf:resource="#Operator"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#positivePreference -->

    <owl:ObjectProperty rdf:about="#positivePreference">
        <rdfs:label xml:lang="en"
            >Positive Optional Preference</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property relates a demand to a preference P in an optional way. Individuals that fulfill the condition P increase their utility value.</rdfs:comment>
        <rdfs:domain rdf:resource="#Demand"/>
        <rdfs:range rdf:resource="#Preference"/>
        <rdfs:subPropertyOf rdf:resource="#refinedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#refinedBy -->

    <owl:ObjectProperty rdf:about="#refinedBy">
        <rdfs:label xml:lang="en">Refined By</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A modal description can be refined by adding new components.</rdfs:comment>
        <rdfs:domain rdf:resource="#ModalDescription"/>
        <rdfs:range rdf:resource="#ModalDescription"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#requiredPreference -->

    <owl:ObjectProperty rdf:about="#requiredPreference">
        <rdfs:label xml:lang="en"
            >Required Preference</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property relates a demand with a preference P in a mandatory way. Individuals that fulfill the condition P are considered valid candidateds for this demand.</rdfs:comment>
        <rdfs:domain rdf:resource="#Demand"/>
        <rdfs:range rdf:resource="#Preference"/>
        <rdfs:subPropertyOf rdf:resource="#refinedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#unionComposite -->

    <owl:ObjectProperty rdf:about="#unionComposite"/>
    


    <!-- http://ontologies.ezweb.morfeo-project.org/reco.owl#unionComposition -->

    <owl:ObjectProperty rdf:about="#unionComposition">
        <rdfs:label xml:lang="en"
            >UNION Composition</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A property which matches each composite-preference with the set of preferences composing it. Being a union-Composition, at least one of the preferences has to be realised.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#CompositePreference"/>
                    <rdf:Description rdf:about="&mIO;CompositeDependency"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Role;Preference"/>
                    <rdf:Description rdf:about="&mIO;Dependency"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


