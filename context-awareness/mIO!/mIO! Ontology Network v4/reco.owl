        <rdfs:label xml:lang="en">Utility Value</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property represents the utility value of a commodity or a service according to the preferences of the user.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


