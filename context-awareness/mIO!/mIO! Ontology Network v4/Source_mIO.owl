    <!-- http://www.cenitmio.es/ontologies/Source.owl#MultipleContextSource -->

    <owl:Class rdf:about="#MultipleContextSource">
        <rdfs:label xml:lang="en"
            >Multiple context source</rdfs:label>
        <rdfs:label xml:lang="es"
            >Fuente de contexto multiple</rdfs:label>
        <owl:equivalentClass>
