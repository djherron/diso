        <rdfs:range rdf:resource="#MultipleContextSource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Source.owl#isContextInformationProvidedBy -->

    <owl:ObjectProperty rdf:about="#isContextInformationProvidedBy">
        <rdfs:label xml:lang="en"
            >isContextInformationProvidedBy</rdfs:label>
        <rdfs:domain rdf:resource="#ContextInformation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Device;Device"/>
                    <rdf:Description rdf:about="&Service;MIOService"/>
                    <rdf:Description rdf:about="#ContextSource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Source.owl#providesContextInformation -->

    <owl:ObjectProperty rdf:about="#providesContextInformation">
        <rdfs:range rdf:resource="#ContextInformation"/>
        <owl:inverseOf rdf:resource="#isContextInformationProvidedBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Device;Device"/>
                    <rdf:Description rdf:about="&Service;MIOService"/>
                    <rdf:Description rdf:about="#ContextSource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


