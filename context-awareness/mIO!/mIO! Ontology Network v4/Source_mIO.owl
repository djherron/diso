        <rdfs:label xml:lang="es"
            >Fuente de contexto</rdfs:label>
        <owl:equivalentClass rdf:resource="#ContextSourceByCardinality"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Source.owl#ContextSourceByCardinality -->

    <owl:Class rdf:about="#ContextSourceByCardinality">
        <rdfs:label xml:lang="en"
            >Context source by cardinality</rdfs:label>
        <rdfs:label xml:lang="es"
            >Fuente de contexto por cardinalidad</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MultipleContextSource"/>
