    <owl:Class rdf:about="#SimpleContextSource">
        <rdfs:label xml:lang="en"
            >Simple context source</rdfs:label>
        <rdfs:label xml:lang="es"
            >Fuente de contexto simple</rdfs:label>
        <rdfs:subClassOf rdf:resource="#ContextSourceByCardinality"/>
    </owl:Class>
    


