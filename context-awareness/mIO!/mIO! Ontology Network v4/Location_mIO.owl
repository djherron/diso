<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY Country "http://www.cenitmio.es/ontologies/Country.owl#" >
    <!ENTITY Location "http://www.cenitmio.es/ontologies/Location.owl#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY space "http://pervasive.semanticweb.org/ont/2004/06/space#" >
    <!ENTITY pol "http://pervasive.semanticweb.org/ont/2004/06/policy#" >
    <!ENTITY context_CODAMOS_mIO "http://www.cenitmio.es/ontologies/context_CODAMOS_mIO.owl#" >
    <!ENTITY geom "http://pervasive.semanticweb.org/ont/2004/06/geo-measurement#" >
    <!ENTITY e_response_buildings "http://e-response.org/ontology/2006/20060815/e_response_buildings#" >
]>


<rdf:RDF xmlns="http://www.cenitmio.es/ontologies/Location.owl#"
     xml:base="http://www.cenitmio.es/ontologies/Location.owl"
     xmlns:geom="http://pervasive.semanticweb.org/ont/2004/06/geo-measurement#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:e_response_buildings="http://e-response.org/ontology/2006/20060815/e_response_buildings#"
     xmlns:pol="http://pervasive.semanticweb.org/ont/2004/06/policy#"
     xmlns:context_CODAMOS_mIO="http://www.cenitmio.es/ontologies/context_CODAMOS_mIO.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:space="http://pervasive.semanticweb.org/ont/2004/06/space#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:Country="http://www.cenitmio.es/ontologies/Country.owl#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:Location="http://www.cenitmio.es/ontologies/Location.owl#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo>3.0</owl:versionInfo>
        <dc:creator
            >Mari Carmen Su&#225;rez-Figueroa</dc:creator>
        <dc:creator
            >Mar&#237;a Poveda-Villal&#243;n</dc:creator>
        <rdfs:comment xml:lang="en"
            >This work has been carried out by the Ontological Engineering Group,  supported by the mIO! project.</rdfs:comment>
        <dc:title xml:lang="en"
            >mIO! Location Ontology</dc:title>
        <rdfs:comment xml:lang="es"
            >Ontolog&#237;a mIO! sobre localizaci&#243;n</rdfs:comment>
        <owl:imports rdf:resource="http://e-response.org/ontology/2006/20060815/e_response_buildings"/>
        <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space"/>
        <owl:imports rdf:resource="http://www.cenitmio.es/ontologies/Country.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasAddress -->

    <owl:ObjectProperty rdf:about="#hasAddress">
        <rdfs:domain rdf:resource="&space;SpatialThing"/>
        <rdfs:range rdf:resource="#Address"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasGeo -->

    <owl:ObjectProperty rdf:about="#hasGeo">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:range rdf:resource="&space;GeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasGeoDirect -->

    <owl:ObjectProperty rdf:about="#hasGeoDirect">
        <rdfs:range rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:domain rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:subPropertyOf rdf:resource="#hasGeo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasLocation -->

    <owl:ObjectProperty rdf:about="#hasLocation">
        <rdfs:domain rdf:resource="&space;SpatialThing"/>
        <rdfs:range rdf:resource="#NAryRelationHasLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasPrecisionDistanceUnit -->

    <owl:ObjectProperty rdf:about="#hasPrecisionDistanceUnit">
        <rdfs:range rdf:resource="&geom;DistanceUnit"/>
        <rdfs:domain rdf:resource="#NAryRelationHasPrecisionMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasPrecisionMeasurement -->

    <owl:ObjectProperty rdf:about="#hasPrecisionMeasurement">
        <rdfs:domain rdf:resource="&geom;LocationCoordinates"/>
        <rdfs:range rdf:resource="#NAryRelationHasPrecisionMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasSpaceType -->

    <owl:ObjectProperty rdf:about="#hasSpaceType">
        <rdfs:domain rdf:resource="&space;GeographicalSpace"/>
        <rdfs:range rdf:resource="#SpaceType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasWay -->

    <owl:ObjectProperty rdf:about="#hasWay">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="#Way"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isInAutonomousRegion -->

    <owl:ObjectProperty rdf:about="#isInAutonomousRegion">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="#AutonomousRegion"/>
        <rdfs:subPropertyOf rdf:resource="#isInGeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isInCity -->

    <owl:ObjectProperty rdf:about="#isInCity">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="#City"/>
        <rdfs:subPropertyOf rdf:resource="#isInGeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isInCountry -->

    <owl:ObjectProperty rdf:about="#isInCountry">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="#Country"/>
        <rdfs:subPropertyOf rdf:resource="#isInGeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isInGeopoliticalEntity -->

    <owl:ObjectProperty rdf:about="#isInGeopoliticalEntity">
        <rdfs:range rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:domain rdf:resource="#Address"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isInPostalDistrict -->

    <owl:ObjectProperty rdf:about="#isInPostalDistrict">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="#PostalDistrict"/>
        <rdfs:subPropertyOf rdf:resource="#isInGeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isInProvince -->

    <owl:ObjectProperty rdf:about="#isInProvince">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="#Province"/>
        <rdfs:subPropertyOf rdf:resource="#isInGeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isLocatedAtGeopoliticalEntity -->

    <owl:ObjectProperty rdf:about="#isLocatedAtGeopoliticalEntity">
        <rdfs:range rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:domain rdf:resource="#NAryRelationHasLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isPartOfGeo -->

    <owl:ObjectProperty rdf:about="#isPartOfGeo">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:range rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:domain rdf:resource="&space;GeopoliticalEntity"/>
        <owl:inverseOf rdf:resource="#hasGeo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isPartOfGeoDirect -->

    <owl:ObjectProperty rdf:about="#isPartOfGeoDirect">
        <rdfs:domain rdf:resource="&space;GeopoliticalEntity"/>
        <rdfs:range rdf:resource="&space;GeopoliticalEntity"/>
        <owl:inverseOf rdf:resource="#hasGeoDirect"/>
        <rdfs:subPropertyOf rdf:resource="#isPartOfGeo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#isSpaceTypeOf -->

    <owl:ObjectProperty rdf:about="#isSpaceTypeOf">
        <rdfs:range rdf:resource="&space;GeographicalSpace"/>
        <rdfs:domain rdf:resource="#SpaceType"/>
        <owl:inverseOf rdf:resource="#hasSpaceType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#obtainedByMethod -->

    <owl:ObjectProperty rdf:about="#obtainedByMethod">
        <rdfs:range rdf:resource="#MethodOfObtainingLocation"/>
        <rdfs:domain rdf:resource="#NAryRelationHasLocation"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#addressFloor -->

    <owl:DatatypeProperty rdf:about="#addressFloor">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#addressLetter -->

    <owl:DatatypeProperty rdf:about="#addressLetter">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#addressNumber -->

    <owl:DatatypeProperty rdf:about="#addressNumber">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#addressPostalCode -->

    <owl:DatatypeProperty rdf:about="#addressPostalCode">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#addressStair -->

    <owl:DatatypeProperty rdf:about="#addressStair">
        <rdfs:domain rdf:resource="#Address"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#hasPrecisionValue -->

    <owl:DatatypeProperty rdf:about="#hasPrecisionValue">
        <rdfs:domain rdf:resource="#NAryRelationHasPrecisionMeasurement"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#wayName -->

    <owl:DatatypeProperty rdf:about="#wayName">
        <rdfs:domain rdf:resource="#Way"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://e-response.org/ontology/2006/20060815/e_response_buildings#NonResidentialBuilding -->

    <owl:Class rdf:about="&e_response_buildings;NonResidentialBuilding"/>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/geo-measurement#DistanceUnit -->

    <owl:Class rdf:about="&geom;DistanceUnit"/>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/geo-measurement#LocationCoordinates -->

    <owl:Class rdf:about="&geom;LocationCoordinates">
        <rdfs:subClassOf rdf:resource="#AbsoluteLocationInformation"/>
    </owl:Class>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/space#FixedStructure -->

    <owl:Class rdf:about="&space;FixedStructure"/>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/space#GeographicalSpace -->

    <owl:Class rdf:about="&space;GeographicalSpace"/>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/space#GeopoliticalEntity -->

    <owl:Class rdf:about="&space;GeopoliticalEntity">
        <rdfs:label xml:lang="en"
            >Geopolitical entity</rdfs:label>
        <rdfs:label xml:lang="es"
            >L&#237;mite administrativo</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AutonomousRegion"/>
                    <rdf:Description rdf:about="#City"/>
                    <rdf:Description rdf:about="#Country"/>
                    <rdf:Description rdf:about="#PostalDistrict"/>
                    <rdf:Description rdf:about="#Province"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Un objeto geopol&#237;tico es una entidad geogr&#225;fica debida a la intervenci&#243;n humana. S&#243;lo puede ser uno de los siguientes elementos: pa&#237;s, comunidad aut&#243;noma, provincia, ciudad o distrito postal. Adem&#225;s, estos elementos son disjuntos entre s&#237;.</rdfs:comment>
    </owl:Class>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/space#MovableSpatialThing -->

    <owl:Class rdf:about="&space;MovableSpatialThing"/>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure -->

    <owl:Class rdf:about="&space;SpaceInAFixedStructure"/>
    


    <!-- http://pervasive.semanticweb.org/ont/2004/06/space#SpatialThing -->

    <owl:Class rdf:about="&space;SpatialThing">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasAddress"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Country.owl#Country -->

    <owl:Class rdf:about="&Country;Country">
        <owl:equivalentClass rdf:resource="#Country"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#AbsoluteLocationInformation -->

    <owl:Class rdf:about="#AbsoluteLocationInformation">
        <rdfs:label xml:lang="en"
            >AbsoluteLocationInformation</rdfs:label>
        <rdfs:subClassOf rdf:resource="#LocationInformation"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Address -->

    <owl:Class rdf:about="#Address">
        <rdfs:label xml:lang="en">Address</rdfs:label>
        <rdfs:label xml:lang="es">Direcci&#243;n</rdfs:label>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Alleway -->

    <owl:Class rdf:about="#Alleway">
        <rdfs:label xml:lang="en">Alleyway</rdfs:label>
        <rdfs:label xml:lang="es">Traves&#237;a</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Way"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#ArchitecturalElement -->

    <owl:Class rdf:about="#ArchitecturalElement">
        <rdfs:label xml:lang="en"
            >ArchitecturalElement</rdfs:label>
        <rdfs:label xml:lang="es"
            >Elemento arquitect&#243;nico</rdfs:label>
        <rdfs:subClassOf rdf:resource="&space;FixedStructure"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#AutonomousRegion -->

    <owl:Class rdf:about="#AutonomousRegion">
        <rdfs:label xml:lang="en"
            >Autonomous region</rdfs:label>
        <rdfs:label xml:lang="es"
            >Comunidad aut&#243;noma</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&space;GeopoliticalEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGeoDirect"/>
                        <owl:someValuesFrom rdf:resource="#Province"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isPartOfGeoDirect"/>
                        <owl:someValuesFrom rdf:resource="#Country"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isPartOfGeoDirect"/>
                <owl:onClass rdf:resource="#Country"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#City"/>
        <owl:disjointWith rdf:resource="#Country"/>
        <owl:disjointWith rdf:resource="#PostalDistrict"/>
        <owl:disjointWith rdf:resource="#Province"/>
        <rdfs:comment
            >Comunidad aut&#243;noma es un objeto geopol&#237;tico que es parte de un solo pa&#237;s y tiene una o varias provincias.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Avenue -->

    <owl:Class rdf:about="#Avenue">
        <rdfs:label xml:lang="en">Avenue</rdfs:label>
        <rdfs:label xml:lang="es">Avenida</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Way"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#BusStation -->

    <owl:Class rdf:about="#BusStation">
        <rdfs:label xml:lang="en">BusStation</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PublicTransportStation"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#City -->

    <owl:Class rdf:about="#City">
        <rdfs:label xml:lang="en">City</rdfs:label>
        <rdfs:label xml:lang="es">Ciudad</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&space;GeopoliticalEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGeoDirect"/>
                        <owl:someValuesFrom rdf:resource="#PostalDistrict"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isPartOfGeoDirect"/>
                        <owl:someValuesFrom rdf:resource="#Province"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isPartOfGeoDirect"/>
                <owl:onClass rdf:resource="#Province"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Country"/>
        <owl:disjointWith rdf:resource="#PostalDistrict"/>
        <owl:disjointWith rdf:resource="#Province"/>
        <rdfs:comment
            >Una ciudad es un objeto geopol&#237;tico que es parte de una o varias provincias (existen ciudades lim&#237;trofes que se encuentran entre dos provincias) y contiene uno o m&#225;s distritos postales.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#ConferenceRoom -->

    <owl:Class rdf:about="#ConferenceRoom">
        <rdfs:label xml:lang="en">Conference room</rdfs:label>
        <rdfs:label xml:lang="es"
            >Sala de congresos</rdfs:label>
        <rdfs:subClassOf rdf:resource="&space;SpaceInAFixedStructure"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Country -->

    <owl:Class rdf:about="#Country">
        <rdfs:label xml:lang="en">Country</rdfs:label>
        <rdfs:label xml:lang="es">Pa&#237;s</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&space;GeopoliticalEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGeoDirect"/>
                        <owl:someValuesFrom rdf:resource="#AutonomousRegion"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#PostalDistrict"/>
        <owl:disjointWith rdf:resource="#Province"/>
        <rdfs:comment
            >Un pa&#237;s es un conjunto de comunidades aut&#243;nomas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Crossroad -->

    <owl:Class rdf:about="#Crossroad">
        <rdfs:label xml:lang="en">Crossroad</rdfs:label>
        <rdfs:label xml:lang="es">Cruce</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Way"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Gymnasium -->

    <owl:Class rdf:about="#Gymnasium">
        <rdfs:label xml:lang="en">Gymnasium</rdfs:label>
        <rdfs:label xml:lang="es">Gimnasio</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SportFacility"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#LocationInformation -->

    <owl:Class rdf:about="#LocationInformation">
        <rdfs:label xml:lang="en"
            >LocationInformation</rdfs:label>
        <rdfs:label xml:lang="es"
            >Informaci&#243;n sobre localizaci&#243;n</rdfs:label>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#MethodOfObtainingLocation -->

    <owl:Class rdf:about="#MethodOfObtainingLocation"/>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#Museum -->

    <owl:Class rdf:about="#Museum">
        <rdfs:label xml:lang="en">Museum</rdfs:label>
        <rdfs:label xml:lang="es">Museo</rdfs:label>
        <rdfs:subClassOf rdf:resource="&e_response_buildings;NonResidentialBuilding"/>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#NAryRelationHasLocation -->

    <owl:Class rdf:about="#NAryRelationHasLocation">
        <rdfs:label xml:lang="en"
            >N-ary relation has location</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Class created to support the nary relation among located entities, coordinates and temporal entities.</rdfs:comment>
        <rdfs:comment xml:lang="es"
            >Punto de extensi&#243;n para enlazar la localizaci&#243;n con el tiempo en la red.

Especificar este naria con &quot;NAryRelationHasLocationAtTime&quot;.

Especificar la relaci&#243;n &quot;hasLocation&quot; con &quot;hasLocationAtTime&quot; que ira de  &quot;NAryRelationHasLocationAtTime&quot; a &quot;TimeInterval U TimeInstant&quot;.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/Location.owl#NAryRelationHasPrecisionMeasurement -->

    <owl:Class rdf:about="#NAryRelationHasPrecisionMeasurement">
        <rdfs:label xml:lang="en"
            >N-ary relation has precision measurement</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Class created to support the nary relation among coordinates measurement and its error.</rdfs:comment>
    </owl:Class>
    


