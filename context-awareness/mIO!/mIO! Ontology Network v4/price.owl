<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY price "http://www.ontologydesignpatterns.org/cp/owl/price.owl#" >
]>


<rdf:RDF xmlns="http://www.ontologydesignpatterns.org/cp/owl/price.owl#"
     xml:base="http://www.ontologydesignpatterns.org/cp/owl/price.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:price="http://www.ontologydesignpatterns.org/cp/owl/price.owl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo rdf:datatype="&xsd;string"
            >Created by Valentina Presutti</owl:versionInfo>
        <rdfs:comment
            >The price content ontology design pattern. This CP represents the price of an entity in a certain currency.</rdfs:comment>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasCurrency -->

    <owl:ObjectProperty rdf:about="#hasCurrency">
        <rdfs:label xml:lang="en">has currency</rdfs:label>
        <rdfs:range rdf:resource="#Currency"/>
        <rdfs:domain rdf:resource="#Price"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice -->

    <owl:ObjectProperty rdf:about="#hasPrice">
        <rdfs:label xml:lang="en">has price</rdfs:label>
        <rdfs:range rdf:resource="#Price"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/price.owl#isPriceOf -->

    <owl:ObjectProperty rdf:about="#isPriceOf">
        <rdfs:label xml:lang="en">is price of</rdfs:label>
        <rdfs:domain rdf:resource="#Price"/>
        <owl:inverseOf rdf:resource="#hasPrice"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasValue -->

    <owl:DatatypeProperty rdf:about="#hasValue">
        <rdfs:label xml:lang="en">has value</rdfs:label>
        <rdfs:domain rdf:resource="#Price"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/price.owl#Currency -->

    <owl:Class rdf:about="#Currency">
        <rdfs:label xml:lang="en">Currency</rdfs:label>
        <owl:disjointWith rdf:resource="#Price"/>
        <rdfs:comment
            >A system of money in general use in a particular country.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/price.owl#Price -->

    <owl:Class rdf:about="#Price">
        <rdfs:label xml:lang="en">Price</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasValue"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCurrency"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >The amount of money expected, required, or given in payment for something.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
</rdf:RDF>



