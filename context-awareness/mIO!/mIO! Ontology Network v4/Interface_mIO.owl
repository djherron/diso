    <!-- http://www.cenitmio.es/ontologies/Interface.owl#MixedInterface -->

    <owl:Class rdf:about="#MixedInterface">
        <rdfs:label xml:lang="en">Mixed interface</rdfs:label>
        <rdfs:label xml:lang="es">Interfaz mixta</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Interface"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasInterfaceType"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Interface"/>
        <rdfs:comment xml:lang="en"
            >A mixed interface is a interface that have al least two interface types.</rdfs:comment>
    </owl:Class>
    


