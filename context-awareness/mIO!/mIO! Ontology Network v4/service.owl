<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY service "http://www.cenitmio.es/ontologies/service.owl#" >
    <!ENTITY base "http://www.cenitmio.es/ontologies/service.owl" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
]>


<rdf:RDF xmlns="&service;"
     xml:base="&base;"
     xmlns:dc="&dc;"
     xmlns:rdfs="&rdfs;"
     xmlns:owl="&owl;"
     xmlns:service="&service;"
     xmlns:xsd="&xsd;"
     xmlns:rdf="&rdf;"
     xmlns:foaf="&foaf;">
     
    <owl:Ontology rdf:about="">
        <dc:date rdf:datatype="&xsd;date">2010-07-02</dc:date>
        <dc:rights xml:lang="en">(c) TreeLogic, 2010</dc:rights>
        <dc:contributor xml:lang="en">Diego Berrueta</dc:contributor>
        <dc:creator xml:lang="en">Ivan Mínnguez</dc:creator>
        <dc:creator xml:lang="en">Luis Polo</dc:creator>
        <dc:contributor xml:lang="en">Marcos Sacristán</dc:contributor>
        <dc:contributor xml:lang="en">Pablo López</dc:contributor>
        <dc:title xml:lang="en">mIO! Services Ontology</dc:title>
    </owl:Ontology>
    


<!--    Annotation properties    -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


<!--    Object Properties     -->

    
    <!-- http://www.cenitmio.es/ontologies/service.owl#implements -->

    <owl:ObjectProperty rdf:about="#implements">
        <rdfs:label xml:lang="en">implements</rdfs:label>
        <rdfs:comment xml:lang="en">Un servicio web implementa una o varias operaciones.</rdfs:comment>
        <rdfs:range rdf:resource="#Operation"/>
        <rdfs:domain rdf:resource="#Service"/>
    </owl:ObjectProperty>
    

    <!-- http://www.cenitmio.es/ontologies/service.owl#messageData -->

    <owl:ObjectProperty rdf:about="#messageData">
        <rdfs:label xml:lang="en">messageData</rdfs:label>
        <rdfs:comment xml:lang="en">Describe el contenido del mensaje</rdfs:comment>
        <rdfs:domain rdf:resource="#Message"/>
    </owl:ObjectProperty>
    

    <!-- http://www.cenitmio.es/ontologies/service.owl#inputMessage -->

    <owl:ObjectProperty rdf:about="#inputMessage">
        <rdfs:label xml:lang="en">inputMessage</rdfs:label>
        <rdfs:comment xml:lang="en">Esta propiedad relaciona un mensaje con la operación del servicio correspondiente (input).</rdfs:comment>
        <rdfs:domain rdf:resource="#Message"/>
        <rdfs:range rdf:resource="#Operation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/service.owl#isReceivedBy -->

    <owl:ObjectProperty rdf:about="#isReceivedBy">
        <rdfs:label xml:lang="en">isReceivedBy</rdfs:label>
        <rdfs:comment xml:lang="en">Esta propiedad relaciona un mensaje con la entidad que lo recibe.</rdfs:comment>
	<rdfs:range>
	    <owl:Class>    
	      <owl:unionOf rdf:parseType="Collection">
		<owl:Class rdf:about="&foaf;Agent"/>
		<owl:Class rdf:about="#Service"/>
	      </owl:unionOf>
	    </owl:Class>  	  
	</rdfs:range>
        <rdfs:domain rdf:resource="#Message"/>
    </owl:ObjectProperty>
    

    <!-- http://www.cenitmio.es/ontologies/service.owl#isSentBy -->

    <owl:ObjectProperty rdf:about="#isSentBy">
        <rdfs:label xml:lang="en"></rdfs:label>
        <rdfs:comment xml:lang="en">Esta propiedad relaciona un mensaje con la entidad que lo envía.</rdfs:comment>
	<rdfs:range>
	    <owl:Class>    
	      <owl:unionOf rdf:parseType="Collection">
		<owl:Class rdf:about="&foaf;Agent"/>
		<owl:Class rdf:about="#Service"/>
	      </owl:unionOf>
	    </owl:Class>  	  
	</rdfs:range>
        <rdfs:domain rdf:resource="#Message"/>
    </owl:ObjectProperty>
    

    <!-- http://www.cenitmio.es/ontologies/service.owl#outputMessage -->

    <owl:ObjectProperty rdf:about="#outputMessage">
        <rdfs:label xml:lang="en">outputMessage</rdfs:label>
        <rdfs:comment xml:lang="en">Esta propiedad relaciona un mensaje con la operación del servicio correspondiente (output).</rdfs:comment>
        <rdfs:domain rdf:resource="#Message"/>
        <rdfs:range rdf:resource="#Operation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/service.owl#postcondition -->

    <owl:ObjectProperty rdf:about="#postcondition">
        <rdfs:label xml:lang="en">postcondition</rdfs:label>
        <rdfs:comment xml:lang="en">Define el tipo de respuesta (output) que genera la operación de un servicio.</rdfs:comment>
        <rdfs:domain rdf:resource="#Operation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/service.owl#precondition -->

    <owl:ObjectProperty rdf:about="#precondition">
        <rdfs:label xml:lang="en">precondition</rdfs:label>
        <rdfs:comment xml:lang="en">Define el tipo de entrada (input) que requiere la operación de un servicio.</rdfs:comment>
        <rdfs:domain rdf:resource="#Operation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/service.owl#receives -->

    <owl:ObjectProperty rdf:about="#receives">
        <rdfs:label xml:lang="en">receives</rdfs:label>
        <rdfs:comment xml:lang="en">Los servicios y agentes reciben mensajes durante la comunicación con otra entidad.</rdfs:comment>
        <rdfs:range rdf:resource="#Message"/>
	<rdfs:domain>
	    <owl:Class>    
	      <owl:unionOf rdf:parseType="Collection">
		<owl:Class rdf:about="&foaf;Agent"/>
		<owl:Class rdf:about="#Service"/>
	      </owl:unionOf>
	    </owl:Class>    
	</rdfs:domain>
        <owl:inverseOf rdf:resource="#isReceivedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cenitmio.es/ontologies/service.owl#sends -->

    <owl:ObjectProperty rdf:about="#sends">
        <rdfs:label xml:lang="en">sends</rdfs:label>
        <rdfs:comment xml:lang="en">Los servicios y agentes envían mensajes durante la comunicación con otra entidad.</rdfs:comment>
        <rdfs:range rdf:resource="#Message"/>
	<rdfs:domain>
	    <owl:Class>    
	      <owl:unionOf rdf:parseType="Collection">
		<owl:Class rdf:about="&foaf;Agent"/>
		<owl:Class rdf:about="#Service"/>
	      </owl:unionOf>
	    </owl:Class>    
	</rdfs:domain>
        <owl:inverseOf rdf:resource="#isSentBy"/>
    </owl:ObjectProperty>
    


<!--    Classes   -->

    

    <!-- http://www.cenitmio.es/ontologies/service.owl#Message -->

    <owl:Class rdf:about="#Message">
        <rdfs:label xml:lang="en">Message</rdfs:label>
        <rdfs:comment xml:lang="en">Un mensaje es la información o el conjunto de datos que se envían desde un emisor a un receptor.</rdfs:comment>
    </owl:Class>
    

    <!-- http://www.cenitmio.es/ontologies/service.owl#Operation -->

    <owl:Class rdf:about="#Operation">
        <rdfs:label xml:lang="en">Operation</rdfs:label>
        <rdfs:comment xml:lang="en">Una operación es una acción o procedimiento que produce un nuevo valor a partir de un conjunto de datos de entrada.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cenitmio.es/ontologies/service.owl#Service -->

    <owl:Class rdf:about="#Service">
        <rdfs:label xml:lang="en">Service</rdfs:label>
        <rdfs:comment xml:lang="en">Un servicio web es un sistema de software diseñado para permitir la interacción interoperable entre máquinas (aplicaciones) sobre una red (W3C).</rdfs:comment>
    </owl:Class>
</rdf:RDF>



