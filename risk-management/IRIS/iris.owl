  <swrl:Imp rdf:ID="CaseStudyRule">
    <swrl:head>
      <swrl:AtomList>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        <rdf:first>
          <swrl:IndividualPropertyAtom>
            <swrl:argument1>
              <swrl:Variable rdf:ID="z"/>
            </swrl:argument1>
            <swrl:argument2>
              <swrl:Variable rdf:ID="y"/>
            </swrl:argument2>
            <swrl:propertyPredicate rdf:resource="#appearsInCaseStudy"/>
          </swrl:IndividualPropertyAtom>
        </rdf:first>
      </swrl:AtomList>
    </swrl:head>
    <swrl:body>
      <swrl:AtomList>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:IndividualPropertyAtom>
                <swrl:propertyPredicate rdf:resource="#appearsInCaseStudy"/>
                <swrl:argument1>
                  <swrl:Variable rdf:ID="x"/>
                </swrl:argument1>
                <swrl:argument2 rdf:resource="#y"/>
              </swrl:IndividualPropertyAtom>
            </rdf:first>
            <rdf:rest>
              <swrl:AtomList>
                <rdf:first>
                  <swrl:IndividualPropertyAtom>
                    <swrl:argument1 rdf:resource="#x"/>
                    <swrl:propertyPredicate rdf:resource="#consistsOf"/>
                    <swrl:argument2 rdf:resource="#z"/>
                  </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </swrl:AtomList>
            </rdf:rest>
          </swrl:AtomList>
        </rdf:rest>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:argument1 rdf:resource="#x"/>
            <swrl:classPredicate rdf:resource="#GroupOfRisks"/>
          </swrl:ClassAtom>
        </rdf:first>
      </swrl:AtomList>
    </swrl:body>
  </swrl:Imp>
  <swrl:Imp rdf:ID="FactorsRule">
    <swrl:body>
      <swrl:AtomList>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:IndividualPropertyAtom>
                <swrl:argument2 rdf:resource="#y"/>
