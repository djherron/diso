                                                                                   ] ,
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor> ;
                                                                                     owl:someValuesFrom [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation>
                                                                                                                               [ rdf:type owl:Restriction ;
                                                                                                                                 owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#precedes> ;
                                                                                                                                 owl:someValuesFrom [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event>
                                                                                                                                                                           [ rdf:type owl:Restriction ;
                                                                                                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#precedes> ;
                                                                                                                                                                             owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation>
                                                                                                                                                                           ]
                                                                                                                                                                         ) ;
                                                                                                                                                      rdf:type owl:Class
                                                                                                                                                    ]
                                                                                                                               ]
                                                                                                                             ) ;
                                                                                                          rdf:type owl:Class
                                                                                                        ]
                                                                                   ] ,
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesTime> ;
                                                                                     owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                                                                     owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval>
                                                                                   ] ,
